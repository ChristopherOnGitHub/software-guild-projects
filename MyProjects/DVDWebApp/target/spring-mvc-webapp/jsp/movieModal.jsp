<div class="modal fade" id="movieModal" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">
                    &times;
                </button>
            </div>
            <div class="modal-body">
                <h3 id="movie-detail-entrynumber"></h3>
                <table id="pet-details-table">
                    <tr>
                        <th style="text-align: right">
                            Title:
                        </th>
                        <td id="movie-detail-title"></td>
                    </tr>
                    <tr>
                        <th style="text-align: right">
                            Release date:
                        </th>
                        <td id="movie-detail-releaseDate"></td>
                    </tr>
                    <tr>
                        <th style="text-align: right">
                            Rating:
                        </th>
                        <td id="movie-detail-rating"></td>
                    </tr>
                    <tr>
                        <th style="text-align: right">
                            Directed by: 
                        </th>
                        <td id="movie-detail-director"></td>
                    </tr>
                    <tr>
                        <th style="text-align: right">
                            Studio: 
                        </th>
                        <td id="movie-detail-studio"></td>
                    </tr>
                    <tr>
                        <th style="text-align: right">
                            User Comments: 
                        </th>
                        <td id="movie-detail-userRating"></td>
                    </tr>
                </table>
            </div>
            <div class="modal-footer">
                <button type="button" class="close" data-dismiss="modal">
                    Close
                </button>
            </div>    
        </div>
    </div>
</div>
<a href="movieModal.jsp"></a>
